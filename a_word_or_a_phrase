import java.io.*;
import java.util.scanner;
public class a_word_or_a_phrase{
public static void main(String []args){
Scanner scan =new Scanner(System.in);
System.out.println("Enter any word or phrase ");
String s[]=new String[];
for(int i=0;i<s[];i++)
{
s[i]=scan.nextLine();
System.out.println(" ",+s[i]);
}
}
}

